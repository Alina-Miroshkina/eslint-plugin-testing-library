{
  "name": "eslint-plugin-testing-library",
  "version": "0.1.0",
  "description": "ESLint rules for Testing Library",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": {
    "name": "Mario Beltrán Alarcón",
    "email": "belco90@gmail.com",
    "url": "https://mario.dev/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Belco90/eslint-plugin-testing-library"
  },
  "files": [
    "docs/",
    "src/"
  ],
  "main": "lib/",
  "scripts": {
    "build": "babel --extensions .js src --out-dir lib --copy-files",
    "postbuild": "rimraf lib/**/__tests__/**",
    "test": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.3.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "eslint": ">=5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": ">=6",
    "npm": ">=5"
  },
  "license": "MIT"
}
